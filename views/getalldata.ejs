<%- include('header') -%>
<h1>halo all data</h1>

<table class="table table-bordered" width="100%" cellspacing="0">
<thead>
    <tr>
        <th>no</th>
        <th>nama</th>
        <th>email</th>
        <th>password</th>
    </tr>
</thead>
<tbody>
    <% for(let i=0;i<data.length;i++){ %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= data[i].nama %></td>
        <td><%= data[i].email %></td>
        <td><%= data[i].password %></td>
        <td class="text-center">
            <form action="/users/<% data[i].id %>?_method=DELETE" method="POST">            <a href="#" class="btn btn-outline-warning btn-sm button-update ">EDIT</a>
            <button type="submit" onclick="return confirm('yakin dihapus?')" class="btn btn-outline-danger btn-sm">Delete</button>
            </form>
        </td>
    </tr>
    <% } %>
</tbody>
</table
>